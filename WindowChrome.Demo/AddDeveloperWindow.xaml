        <TextBox x:Name="textBox3" HorizontalAlignment="Left" Height="23" Margin="127,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="163"/>
        <Label x:Name="label2_Copy3" Content="Official Site" HorizontalAlignment="Left" Margin="8,41,0,0" VerticalAlignment="Top" Height="31" Width="118"/>
        <TextBox x:Name="textBox6" HorizontalAlignment="Left" Height="51" Margin="127,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="163"/>
        <CheckBox x:Name="checkBox" Content="Indie" HorizontalAlignment="Left" Margin="10,77,0,0" VerticalAlignment="Top" Height="18" Width="51"/>
